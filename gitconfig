[push]
	default = simple
[color]
  ui = auto
[merge]
  ff = only
[commit]
  template = ~/.gitmessage
[fetch]
  prune = true
[include]
  path = ~/.gitconfig.local
[status]
	showUntrackedFiles = all
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[credential]
	helper = osxkeychain
